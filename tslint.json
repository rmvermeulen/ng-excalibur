{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "jsdoc-format": false,
    "await-promise": [
      true,
      "Bluebird"
    ],
    "interface-name": false,
    "no-console": false
  },
  "rulesDirectory": []
}
